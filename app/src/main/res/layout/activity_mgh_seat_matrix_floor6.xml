    android:layout_height="wrap_content"
    android:layout_marginLeft="@dimen/_15sdp"
    android:orientation="vertical"
    app:layout_constraintBottom_toTopOf="@+id/btnRoomBook3"
    android:layout_marginBottom="@dimen/_40sdp"
    app:layout_constraintStart_toStartOf="parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <androidx.appcompat.widget.AppCompatButton
            android:layout_width="@dimen/_15sdp"
            android:layout_height="@dimen/_15sdp"
            android:layout_gravity="center"
            android:layout_margin="@dimen/_1sdp"
            android:background="@drawable/room_occupied_full"
            tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:padding="@dimen/_2sdp"
            android:text="   Fully Occupied"
            android:textColor="@color/black"
            android:textSize="@dimen/_15sdp"

            />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <androidx.appcompat.widget.AppCompatButton
            android:layout_width="@dimen/_15sdp"
            android:layout_height="@dimen/_15sdp"
            android:layout_margin="@dimen/_1sdp"
            android:background="@drawable/room_occupied_partially"
            android:textSize="@dimen/_6sdp"
            tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:padding="@dimen/_2sdp"
            android:text="   Partially Occupied"
            android:textColor="@color/black"
            android:textSize="@dimen/_15sdp"
            tools:ignore="SpeakableTextPresentCheck"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <androidx.appcompat.widget.AppCompatButton
            android:layout_width="@dimen/_15sdp"
            android:layout_height="@dimen/_15sdp"
            android:layout_margin="@dimen/_1sdp"
            android:background="@drawable/room_gradient"
            tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:padding="@dimen/_2sdp"
            android:text="   Available"
            android:textColor="@color/black"
            android:textSize="@dimen/_15sdp" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <ImageView
            android:layout_width="19dp"
            android:layout_height="19dp"
            android:layout_margin="@dimen/_1sdp"
            android:background="@drawable/background"
            android:src="@drawable/water_dispenser" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:padding="@dimen/_2sdp"
            android:text="   Water Cooler"
            android:textColor="@color/black"
            android:textSize="@dimen/_15sdp" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <ImageView
            android:layout_width="19dp"
            android:layout_height="19dp"
            android:layout_margin="@dimen/_1sdp"
            android:background="@drawable/background"
            android:src="@drawable/ic_toilet" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:padding="@dimen/_2sdp"
            android:text="   Washroom"
            android:textColor="@color/black"
            android:textSize="@dimen/_15sdp" />
    </LinearLayout>
</LinearLayout>
<androidx.appcompat.widget.AppCompatButton
    android:layout_width="@dimen/_150sdp"
    android:layout_height="wrap_content"
    android:text="Book Room"
    android:background="#000000"
    android:textColor="@color/white"
    android:id="@+id/btnRoomBook3"
    android:layout_marginBottom="@dimen/_40sdp"
    app:layout_constraintBottom_toBottomOf="@+id/scrollView"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent" />

<!--    <TextView-->
<!--        android:id="@+id/display"-->
<!--        android:layout_width="match_parent"-->
<!--        android:layout_height="wrap_content"-->
<!--        android:text="hloooo"-->
<!--        app:layout_constraintBottom_toTopOf="@+id/btnRoomBook3"-->
<!--        app:layout_constraintEnd_toEndOf="parent" />-->
<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginBottom="19dp"
    android:background="@color/white"
    android:fontFamily="@font/audiowide"
    android:padding="@dimen/_5sdp"
    android:text="ROOM PLAN FOR MGH"
    android:textColor="@color/black"
    android:textSize="@dimen/_20sdp"
    android:textStyle="bold"
    app:layout_constraintBottom_toBottomOf="@+id/imageView4"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>



